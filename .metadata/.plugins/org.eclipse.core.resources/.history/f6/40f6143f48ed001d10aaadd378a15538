package day15.api.ramda.basic;

public class MainClass {

	public static void main(String[] args) {
		// 익명 클래스
		
//		new Car(); // 인터페이스이므로 객체 생성 불가
		
		// 선언과 생성을 동시에
		Car car = new Car() { // Car 타입으로 객체 선언 및 생성
			@Override
			public void run() {
				System.out.println("익명 개체로 생성된 car");
			}
		};
		
		car.run();
	}
}
